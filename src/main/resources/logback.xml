<configuration>

  <appender name="FILE" class="ch.qos.logback.core.FileAppender">
    <file>/Users/tymalik/Docs/Git/lark-preprocess/logs/out.log</file>
    <append>true</append>
    <encoder>
      <pattern
            >%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="INFO">
    <appender-ref ref="FILE" />
  </root>

</configuration>
